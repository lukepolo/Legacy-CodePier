{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "bacon/bacon-qr-code": "~1.0",
    "barryvdh/laravel-ide-helper": "^2.4",
    "doctrine/dbal": "^2.6",
    "fideloper/proxy": "~4.0",
    "gentle/bitbucket-api": "^1.1.0",
    "guzzlehttp/guzzle": "~6.2",
    "knplabs/github-api": "^2.6",
    "laravel/cashier": "~7.0",
    "laravel/framework": "5.6.*",
    "laravel/horizon": "^1.0.5",
    "laravel/passport": "5.0.*",
    "laravel/socialite": "^3.0",
    "laravel/tinker": "^1.0",
    "lord/laroute": "^2.4.7",
    "malc0mn/vultr-api-client": "^1.1",
    "mpociot/teamwork": "~5.1",
    "php-http/guzzle6-adapter": "^1.1",
    "phpseclib/phpseclib": "^2.0",
    "pragmarx/google2fa": "^3.0",
    "predis/predis": "^1.1",
    "pusher/pusher-php-server": "^3.0",
    "sentry/sentry-laravel": "^0.8.0",
    "socialiteproviders/digitalocean": "^3.0",
    "socialiteproviders/gitlab": "^3.0.1",
    "socialiteproviders/slack": "^3.0",
    "spatie/laravel-newsletter": "^4.2",
    "stevenmaguire/oauth2-bitbucket": "^2.0",
    "toin0u/digitalocean-v2": "^2.2",
    "vinkla/hashids": "^5.0.0"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "friendsofphp/php-cs-fixer": "^2.10",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "nunomaduro/collision": "~2.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "classmap": ["database"],
    "psr-4": {
      "App\\": "app/"
    },
    "files": ["app/helpers.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": ["@php artisan key:generate"],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
