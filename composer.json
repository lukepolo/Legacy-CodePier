{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        { "type": "vcs", "url": "https://github.com/lukepolo/dbal" },
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        { "packagist": false }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "predis/predis": "^1.1",
        "barryvdh/laravel-debugbar": "^2.2",
        "filp/whoops": "^2.1",
        "barryvdh/laravel-ide-helper": "^2.1",
        "doctrine/dbal": "dev-mysql57-json",
        "laravel/socialite": "^2.0",
        "socialiteproviders/digitalocean": "^2.0",
        "graham-campbell/digitalocean": "^3.1",
        "graham-campbell/github": "^4.4",
        "phpseclib/phpseclib": "^2.0",
        "authy/php": "2.*",
        "pda/pheanstalk": "^3.1",
        "mpociot/teamwork": "~3.0",
        "laravel/cashier": "~7.0",
        "dompdf/dompdf" : "~0.6",
        "gentle/bitbucket-api": "^0.8.1",
        "vinkla/hashids": "^2.4",
        "socialiteproviders/slack": "^2.0",
        "tomschlick/laravel-http2-server-push": "^0.1.2",
        "pusher/pusher-php-server": "^2.4",
        "guzzlehttp/guzzle": "~6.0",
        "m4tthumphrey/php-gitlab-api": "^7.13",
        "socialiteproviders/gitlab": "^2.0",
        "lord/laroute" : "^2.3.0",
        "laravel/passport": "1.0.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
