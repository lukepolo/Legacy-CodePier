{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        { "type": "vcs", "url": "https://github.com/lukepolo/dbal" }
    ],
    "require": {
        "php": ">=7.0.0",
        "bacon/bacon-qr-code": "~1.0",
        "barryvdh/laravel-ide-helper": "^2.3",
        "doctrine/dbal": "dev-mysql57-json",
        "dompdf/dompdf": "~0.8",
        "fideloper/proxy": "~3.3",
        "filp/whoops": "^2.1",
        "gentle/bitbucket-api": "^0.8.2",
        "graham-campbell/digitalocean": "^4.0",
        "graham-campbell/github": "^6.0",
        "guzzlehttp/guzzle": "~6.2",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.5.*",
        "laravel/passport": "3.0.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "lord/laroute": "^2.4.4",
        "mpociot/teamwork": "~5.0",
        "pda/pheanstalk": "^3.1",
        "php-http/guzzle6-adapter": "^1.1",
        "phpseclib/phpseclib": "^2.0",
        "pragmarx/google2fa": "^2.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.0",
        "sentry/sentry-laravel": "^0.8.0",
        "socialiteproviders/digitalocean": "^3.0",
        "socialiteproviders/gitlab": "^3.0.1",
        "socialiteproviders/slack": "^3.0",
        "vinkla/hashids": "^3.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.6",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
