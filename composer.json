{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        { "type": "vcs", "url": "https://github.com/lukepolo/dbal" },
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        { "packagist": false }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.4.*",
        "predis/predis": "^1.1",
        "filp/whoops": "^2.1",
        "barryvdh/laravel-ide-helper": "^2.2",
        "doctrine/dbal": "dev-mysql57-json",
        "laravel/socialite": "^3.0",
        "socialiteproviders/digitalocean": "^3.0",
        "graham-campbell/digitalocean": "^3.2",
        "phpseclib/phpseclib": "^2.0",
        "authy/php": "2.*",
        "pda/pheanstalk": "^3.1",
        "mpociot/teamwork": "~5.0",
        "laravel/cashier": "~7.0",
        "dompdf/dompdf" : "~0.7",
        "gentle/bitbucket-api": "^0.8.2",
        "vinkla/hashids": "^3.1",
        "socialiteproviders/slack": "^3.0",
        "pusher/pusher-php-server": "^2.6",
        "guzzlehttp/guzzle": "~6.0",
        "m4tthumphrey/php-gitlab-api": "^7.15",
        "lord/laroute" : "^2.4.1",
        "laravel/passport": "2.0.*",
        "sentry/sentry-laravel": "^0.6.0",
        "graham-campbell/github": "^5.1",
        "php-http/guzzle6-adapter": "^1.1",
        "socialiteproviders/gitlab": "^3.0.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
